{
  "name": "@readyplayerme/react-avatar-creator",
  "version": "0.5.0",
  "description": "Ready Player Me - React Avatar Creator",
  "keywords": [
    "readyplayerme",
    "avatar",
    "react",
    "typescript",
    "glb"
  ],
  "license": "MIT",
  "author": "Ready Player Me",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "analyze": "size-limit --why",
    "build": "rollup -c",
    "lint": "dts lint",
    "prepare": "rollup -c",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 400,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@rollup/plugin-typescript": "^11.1.2",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@tsconfig/recommended": "^1.0.2",
    "@tsconfig/vite-react": "^1.0.1",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "babel-loader": "^9.1.2",
    "dts-cli": "^2.0.2",
    "husky": "^8.0.3",
    "react": ">=17.0",
    "react-dom": ">=17.0",
    "react-is": "^18.2.0",
    "rollup": "^3.26.0",
    "rollup-plugin-dts": "^5.3.0",
    "size-limit": "^8.2.4",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1"
  },
  "peerDependencies": {
    "react": ">=16"
  }
}
